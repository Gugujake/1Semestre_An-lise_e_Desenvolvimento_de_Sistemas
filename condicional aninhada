#include <stdio.h>
int main()
{
    int num;
    printf ("Digite um n°: ");
    scanf ("%i", &num);
    if (num > 21){
     printf("Triângulo");
    }
    else{
        if (num == 21){
            printf ("São iguais");
        }
        else{
        printf ("Quadrado");
        }
    }
    return 0;
}
